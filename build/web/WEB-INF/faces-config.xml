<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>books</from-outcome>
            <to-view-id>/books.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loan</from-outcome>
            <to-view-id>/loan.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>members</from-outcome>
            <to-view-id>/members.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/books.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>book</from-outcome>
            <to-view-id>/book.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/book.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>bookDelete</from-outcome>
            <to-view-id>/bookDelete.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/loan.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>bookStatus</from-outcome>
            <to-view-id>/loanSelectStatus.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/members.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>memberDelete</from-outcome>
            <to-view-id>/memberDelete.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/memberDelete.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>memberDeleteError</from-outcome>
            <to-view-id>/memberDeleteError.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/memberDeleteError.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>members</from-outcome>
            <to-view-id>/members.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/loanSelectStatus.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loanError</from-outcome>
            <to-view-id>/loanError.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loanSuccessful</from-outcome>
            <to-view-id>/loanSuccessful.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/loanError.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loanSelectStatus</from-outcome>
            <to-view-id>/loanSelectStatus.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/loanSuccessful.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loan</from-outcome>
            <to-view-id>/loan.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
